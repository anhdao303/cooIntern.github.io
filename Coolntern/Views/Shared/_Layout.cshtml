<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/img/favicon.ico">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">

    <!-- CSS here -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/flaticon.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/price_rangs.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/slicknav.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/animate.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/magnific-popup.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/fontawesome-all.min.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/themify-icons.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/slick.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/nice-select.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/style.css" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/css/StyleSheet1.css" type="text/css" media="all" />

    <script src="~/Content/js/vendor/jquery-1.12.4.min.js"></script>

</head>
<body>
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    @{ Html.RenderAction("getLogo", "Temp");}
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    <!-- Preloader Start -->
    <header>
        @{ Html.RenderAction("getFullHeader", "Temp");}
    </header>
    <div class="container-fluid body-content">
        <main>
            @RenderBody()
        </main>
        <hr />
        <footer>
            <!-- Footer Start-->
            <div class="footer-area footer-bg footer-padding">
                <div class="container">
                    @{ Html.RenderAction("getFooter", "Temp");}
                    <!--  -->
                    <div class="row footer-wejed justify-content-between">
                        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
                            <!-- logo -->
                            <div class="footer-logo mb-20">
                                @{ Html.RenderAction("getLogo", "Temp");}
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-5">
                            <div class="footer-tittle-bottom">
                                <span>5000+</span>
                                <p>Talented Hunter</p>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-5">
                            <div class="footer-tittle-bottom">
                                <span>451</span>
                                <p>Talented Hunter</p>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 col-md-4 col-sm-5">
                            <!-- Footer Bottom Tittle -->
                            <div class="footer-tittle-bottom">
                                <span>568</span>
                                <p>Talented Hunter</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- footer-bottom area -->
            <div class="footer-bottom-area footer-bg">
                <div class="container">
                    <div class="footer-border">
                        <div class="row d-flex justify-content-between align-items-center">
                            <div class="col-xl-10 col-lg-10 ">
                                <div class="footer-copy-right">
                                    <p>
                                        Copyright &copy;
                                        <script>document.write(new Date().getFullYear());</script>
                                    </p>
                                </div>
                            </div>
                            <div class="col-xl-2 col-lg-2">
                                <div class="footer-social f-right">
                                    <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    <a href="#"><i class="fab fa-twitter"></i></a>
                                    <a href="#"><i class="fas fa-globe"></i></a>
                                    <a href="#"><i class="fab fa-behance"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Footer End-->
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("_Header", false)
    @RenderSection("_Footer", false)


    <!-- JS here -->
    <!-- All JS Custom Plugins Link Here here -->
    <script src="~/Content/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="~/Content/js/popper.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="~/Content/js/jquery.slicknav.min.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="~/Content/js/owl.carousel.min.js"></script>
    <script src="~/Content/js/slick.min.js"></script>
    <script src="~/Content/js/price_rangs.js"></script>

    <!-- One Page, Animated-HeadLin -->
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/animated.headline.js"></script>
    <script src="~/Content/js/jquery.magnific-popup.js"></script>

    <!-- Scrollup, nice-select, sticky -->
    <script src="~/Content/js/jquery.scrollUp.min.js"></script>
    <script src="~/Content/js/jquery.nice-select.min.js"></script>
    <script src="~/Content/js/jquery.sticky.js"></script>

    <!-- contact js -->
    <script src="~/Content/js/contact.js"></script>
    <script src="~/Content/js/jquery.form.js"></script>
    <script src="~/Content/js/jquery.validate.min.js"></script>
    <script src="~/Content/js/mail-script.js"></script>
    <script src="~/Content/js/jquery.ajaxchimp.min.js"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="~/Content/js/plugins.js"></script>
    <script src="~/Content/js/main.js"></script>
</body>
</html>
